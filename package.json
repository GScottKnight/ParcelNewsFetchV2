{
  "name": "parcel-news-fetch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:site && npm run build:types",
    "build:site": "rm -rf dist && mkdir -p dist && cp -R public/* dist/",
    "build:types": "tsc -p tsconfig.json",
    "dev": "node --watch dist/server/index.js",
    "start": "node dist/server/index.js",
    "serve": "node dist/server/server.js",
    "lint": "echo \"lint not configured yet\""
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/pg": "^8.15.6",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "fast-xml-parser": "^5.3.2",
    "pg": "^8.16.3"
  }
}
